   "source": [
    "class Solution:\n",
    "    def reverse(self, x: int) -> int:\n",
    "        reverse= int(str(abs(x))[::-1])\n",
    "        if reverse >2147483647:\n",
    "            return 0\n",
    "        return reverse if x>0 else (reverse *-1)"
   ]
